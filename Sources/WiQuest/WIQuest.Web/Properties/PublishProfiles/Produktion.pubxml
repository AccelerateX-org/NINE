<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://wiquest.wi.hm.edu</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>https://wiquest.wi.hm.edu:8172/msdeploy.axd</MSDeployServiceURL>
    <DeployIisAppPath>wiquest</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>WIQuest-Admin</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="QuestDb" Order="1" Enabled="False">
          <Destination Path="Data Source=MYSTIK;Initial Catalog=WIQuest.QuestDB;Integrated Security=False;User ID=nine-db;Password=nine-db" Name="Server=MYSTIK;Database=WIQuest.QuestDB;Integrated Security=False;User Id=nine-db;Password=nine-db" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="WIQuest.Web.Data.QuestDbContext, WIQuest.Web" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="UserDb" Order="2" Enabled="False">
          <Destination Path="Data Source=MYSTIK;Initial Catalog=WIQuest.UserDB;Integrated Security=False;User ID=nine-db;Password=nine-db" Name="Server=MYSTIK;Database=WIQuest.UserDB;Integrated Security=False;User Id=nine-db;Password=nine-db" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="WIQuest.Web.Models.UserDbContext, WIQuest.Web" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="LogDbContext" Order="3" Enabled="False">
          <Destination Path="Data Source=MYSTIK;Initial Catalog=WIQuest.LogDB;Integrated Security=False;User ID=nine-db;Password=nine-db" Name="Server=MYSTIK;Database=WIQuest.LogDB;Integrated Security=False;User Id=nine-db;Password=nine-db" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="WIQuest.Web.Models.LogDbContext, WIQuest.Web" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)LogDbContext-Web.config Connection String">
      <ParameterValue>Server=MYSTIK;Database=WIQuest.LogDB;Integrated Security=False;User Id=nine-db;Password=nine-db</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)QuestDb-Web.config Connection String">
      <ParameterValue>Server=MYSTIK;Database=WIQuest.QuestDB;Integrated Security=False;User Id=nine-db;Password=nine-db</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)UserDb-Web.config Connection String">
      <ParameterValue>Server=MYSTIK;Database=WIQuest.UserDB;Integrated Security=False;User Id=nine-db;Password=nine-db</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>