@model MyStik.TimeTable.Web.Models.NewsletterCharacteristicModel
@{
    ViewBag.Title = "Abonennten";
}

<h4>Abonennten von Newsletter @Model.Newsletter.Name</h4>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">Teilnehmer</div>
            <div class="panel-body">
                <table class=" table table-striped table-bordered table-condensed">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Vorname</th>
                            <th>Studienprogramm</th>
                            <th>Studiengruppe</th>
                            <th>Datum Eintragung</th>
                        </tr>
                    </thead>
                    <tbody id="course-subscriptions">
                        @foreach (var member in Model.Member)
                        {
                            <tr id="@member.User.Id">
                                <td>@member.Number</td>
                                <td>@member.User.LastName</td>
                                <td>@member.User.FirstName</td>
                                <td>@member.User.Curriculum</td>
                                <td>@member.User.Group</td>
                                <td>@member.Subscription.TimeStamp</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


