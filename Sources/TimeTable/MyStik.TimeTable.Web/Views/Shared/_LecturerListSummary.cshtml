@model ICollection<MyStik.TimeTable.Web.Models.LecturerViewModel>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var lec in Model)
        {
            <tr>
                <td><a href="@Url.Action("Member", "Organiser", new {orgId = lec.Lecturer.Organiser.ShortName, shortName = lec.Lecturer.ShortName})">@lec.Lecturer.Name</a></td>
                @if (string.IsNullOrEmpty(lec.Lecturer.UrlProfile))
                {
                    <td></td>
                }
                else
                {
                    <td><a href="@lec.Lecturer.UrlProfile" target="_blank">@lec.Lecturer.UrlProfile</a> <i class="fa fa-external-link"></i></td>
                }
                
            </tr>
        }
    </tbody>
</table>


