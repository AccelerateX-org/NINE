@model MyStik.TimeTable.Web.Models.ThesisStateModel
<!DOCTYPE html>
<html lang="de">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<title>Notenmeldung Abschlussarbeit</title>
	<style>
		body {
			font-family: Helvetica,Arial,Sans-Serif;
			font-size: 11px;
			line-height: 1.4;
		}

		table, td {
			border: 1px solid black;
			border-collapse: collapse;
		}

		td {
			padding: 3px;
		}

		.vordruck {
			font-family: Helvetica,Arial,Sans-Serif;
			font-size: 9px;
			line-height: 1.1;
			margin-bottom: 60px;
		}

		.vordruck2 {
			font-family: Helvetica,Arial,Sans-Serif;
			font-size: 9px;
			line-height: 1.1;
			margin-bottom: 5px;
		}
		


	</style>
</head>

<body>
	<table style="width: 100%">
		<tr>
			<td style="width: 33%; vertical-align: top;">
				<div class="vordruck">
					Eingang<br>Bereich Prüfung und Praktikum
				</div>
			</td>
			<td style="width:33%;vertical-align: text-top;">
				<div class="vordruck">
					Erfasst und erledigt
				</div>
			</td>
			<td rowspan="2" style="width: 33%">
				<img style="margin-left: 10pt"
					 src="data:image/gif;base64,">
				<br>
				Hochschule München
				<br />
				@Model.Thesis.Student.Curriculum.Organiser.Name
			</td>
		</tr>
		<tr>
			<td>
				<div class="vordruck2">
					Datum, Handzeichen
				</div>
			</td>
			<td>
				<div class="vordruck2">
					Datum, Handzeichen
				</div>
			</td>
		</tr>
		<tr>
			<td colspan="3" style="text-align: center;">
				<h1>Beleg für das Erfassen der Abschlussarbeit</h1>
			</td>
		</tr>
		<tr>
			<td colspan="3" style="text-align: left;">
				<h2>@Model.User.LastName,  @Model.User.FirstName</h2>
			</td>
		</tr>
		<tr>
			<td colspan="3" style="text-align: left;">
				<div class="vordruck2">
					Name, Vorname des/der Studierenden
				</div>
			</td>
		</tr>
		<tr>
			<td colspan="2" style="text-align: left;">
				<h2>
					@Model.Thesis.Student.Curriculum.Name (@Model.Thesis.Student.Curriculum.ShortName)
				</h2>
			</td>
			<td style="text-align: left;">
				<h1>@Model.Thesis.Student.Number</h1>
			</td>
		</tr>
		<tr>
			<td colspan="2" style="text-align: left;">
				<div class="vordruck2">
					Studiengang
				</div>
			</td>
			<td style="text-align: left;">
				<div class="vordruck2">
					Matrikelnummer
				</div>
			</td>
		</tr>
		<tr>
			<td colspan="2">
				<h1>Note der Abschlussarbeit</h1>
			</td>
			<td>
				<h1>________</h1>
			</td>
		</tr>
		<tr>
			<td colspan="3" style="vertical-align: text-top;">
				<h2>Thema in Deutsch</h2>
				<div>@Model.Thesis.TitleDe</div>
			</td>
		</tr>
		<tr>
			<td colspan="3" style="vertical-align: text-top;">
				<h2>Thema in Englisch</h2>
				<div>@Model.Thesis.TitleEn</div>
			</td>
		</tr>
		@if (Model.Thesis.Supervisors.Count > 1)
		{
			<tr>
				<td style="width: 33%;">
					<div style="margin-top: 40px"></div>
				</td>
				<td style="width: 33%;">
					<div style="margin-top: 40px">
						@if (Model.PrimarySupervisor.Member != null)
						{
							<div> @Model.PrimarySupervisor.Member.FullName </div>
						}
					</div>
				</td>
				<td style="width: 33%">
					<div style="margin-top: 40px"></div>
				</td>
			</tr>
			<tr>
				<td style="width: 33%;">
					<div class="vordruck2">
						Datum ErstprüferIn
					</div>
				</td>
				<td style="width: 33%;">
					<div class="vordruck2">Name ErstprüferIn</div>
				</td>
				<td style="width: 33%">
					<div class="vordruck2">Unterschrift ErstprüferIn</div>
				</td>
			</tr>
			<tr>
				<td style="width: 33%;">
					<div style="margin-top: 40px"></div>
				</td>
				<td style="width: 33%;">
					<div style="margin-top: 40px">
						@if (Model.SecondarySupervisor != null && Model.SecondarySupervisor.Member != null)
						{
							<div>@Model.SecondarySupervisor.Member.FullName</div>
						}
					</div>
				</td>
				<td style="width: 33%">
					<div style="margin-top: 40px"></div>
				</td>
			</tr>
			<tr>
				<td style="width: 33%;">
					<div class="vordruck2">Datum ZweitprüferIn</div>
				</td>
				<td style="width: 33%;">
					<div class="vordruck2">Name ZweitprüferIn</div>
				</td>
				<td style="width: 33%">
					<div class="vordruck2">Unterschrift ZweitprüferIn</div>
				</td>
			</tr>
		}
		else
		{
			<tr>
				<td style="width: 33%;">
					<div style="margin-top: 40px"></div>
				</td>
				<td style="width: 33%;">
					<div style="margin-top: 40px">
						@Model.PrimarySupervisor.Member.FullName
					</div>
				</td>
				<td style="width: 33%">
					<div style="margin-top: 40px"></div>
				</td>
			</tr>
			<tr>
				<td style="width: 33%;">
					<div class="vordruck2">
						Datum PrüferIn
					</div>
				</td>
				<td style="width: 33%;">
					<div class="vordruck2">Name PrüferIn</div>
				</td>
				<td style="width: 33%">
					<div class="vordruck2">Unterschrift PrüferIn</div>
				</td>
			</tr>
		}
		<tr>
			<td colspan="3">
				<div style="margin-top: 40px"></div>
			</td>
		</tr>
		<tr>
			<td colspan="3">
				<div class="vordruck2">Besondere Vorkommnisse (z.B. Erteilung der Fiktionsnote "5" wegen nicht fristgerechter Abgabe)</div>
			</td>
		</tr>
		<tr>
			<td style="width: 33%;">
				<div style="margin-top: 40px"></div>
			</td>
			<td style="width: 33%;">
				<div style="margin-top: 40px"></div>
			</td>
			<td style="width: 33%">
				<div style="margin-top: 40px"></div>
			</td>
		</tr>
		<tr>
			<td style="width:33%;">
				<div class="vordruck2">Datum Vorsitzende/in der Prüfungskommission</div>
			</td>
			<td style="width:33%;">
				<div class="vordruck2">Name Vorsitzende/in der Prüfungskommission</div>
			</td>
			<td style="width:33%">
				<div class="vordruck2">Unterschrift Vorsitzende/in der Prüfungskommission</div>
			</td>
		</tr>

	</table>
</body>

</html>