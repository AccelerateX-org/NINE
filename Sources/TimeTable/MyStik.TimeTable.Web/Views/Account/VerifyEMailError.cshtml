@model MyStik.TimeTable.Web.Models.VerifyEMailModel
@{
    Layout = "~/Views/Shared/_LayoutNewPublic.cshtml";
    ViewBag.Title = "Bestätigung E-Mail Adresse";
}


<div class="row">
    <div class="col-lg-offset-4 col-lg-4 col-md-offset-3 col-md-6 col-sm-offset-2 col-sm-8">
        <div class="panel panel-danger">
            <div class="panel-heading">
                <h4 class="panel-title">Registrierung fehlgeschlagen</h4>
            </div>
            <div class="panel-body">
                <h2>Die Verifikation Ihrer E-Mail Adresse @Model.EMail ist fehlgeschlagen.</h2>

                <p>Grund: @Model.Message</p>
                <p>Für Rückfragen wenden Sie sich an die Ansprechpartner @Html.ActionLink("Kontakt", "Contact", "Home")</p>
            </div>
        </div>
    </div>
</div>



