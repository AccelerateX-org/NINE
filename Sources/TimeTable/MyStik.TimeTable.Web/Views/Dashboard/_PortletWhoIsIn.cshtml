@using MyStik.TimeTable.Web.Helpers
@model MyStik.TimeTable.Web.Models.DashboardViewModel


<div class="portlet portlet-sortable light bordered">
        <div class="portlet-title">
            <div class="caption font-green-sharp">
                <i class="icon-graduation-cap font-green-sharp"></i>
                <span class="caption-subject bold uppercase"> Wer ist im Haus?</span>
            </div>
        </div>
    <div class="portlet-body">
        <table class="table table-condensed">
            <thead>
                <tr>
                    <th>Bezeichnung</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var member in Model.ActiveMembers)
                {
                    <tr>
                        <td>
                            @member.Name
                        </td>
                        <td>
                        </td>
                        <td>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

