@model MyStik.TimeTable.Web.Models.CustomBodyEmail

Content-Type: text/html; charset=utf-8

<!DOCTYPE html>
<html>
<head>
    <meta content="de" http-equiv="Content-Language">
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
    <title></title>
    <style type="text/css">
        .mail-warning-style {
            font-family: Arial, Helvetica, sans-serif;
            color: #0000FF;
        }

        .mail-text-style {
            font-family: Arial, Helvetica, sans-serif;
        }

        .mail-stationary-style {
            font-family: Arial, Helvetica, sans-serif;
            font-size: xx-small;
        }
    </style>
</head>
<body>
<div class="mail-text-style">
    @Html.Raw(Model.Body)
    <hr/>
</div>
    <div class="mail-stationary-style">Dies ist eine manuell erstellte E-Mail. Bei Fragen zum Inhalt wenden Sie sich bitte an den Absender.</div>
    <div class="mail-stationary-style">
        Missbrauch melden: Sollten Sie diese E-Mail fälschlicherweise erhalten haben, melden Sie dies bitte an den <a href="mailto:itsupport@wi.hm.edu">technischen Support</a> von nine.
    </div>
</body>
</html>


