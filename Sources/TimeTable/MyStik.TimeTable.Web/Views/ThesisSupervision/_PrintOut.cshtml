@model MyStik.TimeTable.Web.Models.ThesisEmail

<!DOCTYPE html>
<html lang="de">
<head>
    <title>Hochschule München, Notenmeldung Abschussarbeit</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="descripxion" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        body {
            font-family: Helvetica,Arial,Sans-Serif;
            font-size: 11px;
            line-height: 1.4;
        }

        h1 {
            font-size: 100%;
            margin-top: 0;
        }

            h1.abschnitt {
                margin-top: 30px;
            }

            /* roter Wuerfel */
            h1 img {
            }

        main {
            display: block;
        }

        .ausdrucken {
            margin: 5px 0;
            border: 1px dotted #727272;
            padding: 1px 5px;
        }

        .tip {
            font-style: italic;
            color: #464646;
            margin-bottom: 10px;
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        td {
        }

            td.first {
                width: 70%;
            }

            td.second {
                text-align: right;
            }

        h3 {
            font-size: 100%;
            font-weight: normal;
        }

        h2 {
            font-weight: normal;
            margin-bottom: 1em;
        }

        p {
            margin: 0;
        }

            p.wichtig {
                font-weight: bold;
            }

        .familienname {
            font-weight: bold;
        }

        .anmeldungsnummer {
            border: 3px solid #727272;
            width: 126px;
            height: 80px;
            text-align: center;
            font-size: 30px;
            line-height: 1;
            color: #727272;
            padding: 10px 0 0 0;
        }

        .laufendeNummer {
            color: #000;
            font-weight: bold;
        }

        .bestaetigung {
            border-top: 1px solid #727272;
            margin-top: 1em;
            padding-top: 1em;
        }

        .unterschrift-zeile {
            margin: 3em 0;
        }

            .unterschrift-zeile span {
                border-top: 1px solid #727272;
                padding-right: 12em;
                padding-top: .3em;
            }

        .ort_datum {
            margin-right: 7.5em;
        }

        .mappeZurueck .ort_datum {
            margin-right: 5em;
        }

        .mappeZurueck .unterschrift-zeile {
            margin: 3em 0 0 0;
        }
    </style>

</head>

<body>

    <main>

        <h1>
            <img src="data:image/gif;base64,"> Hochschule München | @Model.Thesis.Student.Curriculum.Organiser.Name
        </h1>

        <h1 class="abschnitt">Notenmeldung Abschlussarbeit</h1>


        <div class="ausdrucken">
            <table>
                <tr>
                    <td class="first">
                        <h3>HM | @Model.Thesis.Student.Curriculum.Organiser.ShortName | @Model.Thesis.Student.Curriculum.ShortName</h3>
                        <h2>
                            Angaben zur Abschlussarbeit
                        </h2>
                        <p>Familienname: @Model.Thesis.User.LastName</p>
                        <p>Vorname: @Model.Thesis.User.FirstName</p>
                        <p>Studiengang: @Model.Thesis.Student.Curriculum.Name</p>
                        <p>Matrikelnummer: @Model.Thesis.Student.Number</p>
                        <p>Titel (deutsch): @Model.Thesis.Thesis.TitleDe</p>
                        <p>Titel (englisch): @Model.Thesis.Thesis.TitleEn</p>
                        <p>Betreuung:</p>
                            @foreach (var s in Model.Thesis.Thesis.Supervisors)
                            {
                                if (s.AcceptanceDate != null)
                                {
                                    <p>@s.Member.FullName</p>
                                }
                                else
                                {
                                    <p>@s.Member.FullName (unbestätigt)</p>
                                }
                            }
                    </td>
                    <td>
                        @if (!string.IsNullOrEmpty(Model.Thesis.Mark))
                        {
                            <div class="anmeldungsnummer">Note<br><span class="laufendeNummer">@Model.Thesis.Mark</span></div>
                        }
                        else
                        {
                            <div class="anmeldungsnummer">Note<br><span class="laufendeNummer">____</span></div>
                        }
                    </td>
                </tr>
            </table>
        </div><!-- /ausdrucken -->



        <div class="ausdrucken">
            <div class="bestaetigung">
                <p class="unterschrift-zeile"><span class="ort_datum">Ort, Datum</span> <span>Unterschrift PrüferIn</span></p>
            </div><!-- /bestaetigung -->

            <div class="bestaetigung mappeZurueck">
                <p class="unterschrift-zeile"><span class="ort_datum">München, Datum</span> <span>Unterschrift PKV</span></p>
            </div><!-- /bestaetigung -->

        </div><!-- /ausdrucken -->

    </main>

</body>

</html>


