@model ICollection<MyStik.TimeTable.Web.Models.UserAdminViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Startseite Systemtests</h2>

<a href="@Url.Action("CreateTestUser")" class="btn btn-primary">Testbenutzer anlegen</a>


<table class="table table-sm">
    <thead>
    <tr>
        <th>Name</th>
        <th>Studienangebot</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.User.UserName</td>
            @if (user.Student != null)
            {
                <td>@user.Student.Curriculum.ShortName</td>
            }
            else
            {
                <td></td>
            }
        </tr>
    }
    </tbody>
</table>

