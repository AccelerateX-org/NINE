@model ICollection<MyStik.TimeTable.Web.Models.UserAdminViewModel>

<table class="table table-sm">
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Vorname</th>
            <th><i class="bi bi-envelope"></i></th>
            <th><i class="bi bi-person-bounding-box"></i></th>
            <th>Registriert</th>
            <th>Letztes Login</th>
            <th>NINE User Account</th>
            <th>HM User Account</th>
            <th>Ablaufdatum</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @{ var i = 0;}
    @foreach (var user in Model)
    {
        i++;
        <tr id="@user.User.Id">
            <td>@i</td>
            @Html.Partial("_UserListEntry", user)
        </tr>
    }
    </tbody>

</table>
